/ {

	fragment@model {
		target-path = "/";
		__overlay__ {
			ssp {
				compatible = "ssp";

				ssp-acc-position = <1>;
				ssp-mag-position = <6>;
				ssp-mag-check-nfc = <&pio 52 0>;
				ssp-prox-thresh-addval = /bits/ 16 <40 20 30>;
				ssp-mag-array = /bits/ 16 <10377 728 (-228) 467 10068 (-165) (-113) 122 11052>;
				ssp-mag-nfc-array = /bits/ 16 <10313 118 (-132) (-38) 9643 (-181) (-175) (-38) 10599>;
				ssp-light-position = /bits/ 32 <42 54 8 27 2 8>;
				ssp-brightness-array-len = <5>;
				ssp-brightness-array = /bits/ 32 <15 40 50 77 255>;
				ssp-light-cam-lux = /bits/ 32 <100 200>;
				ssp-light-cam-br = /bits/ 32 <60 60>;
			};
		};
	};
};

