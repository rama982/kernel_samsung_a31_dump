&i2c5 {
	status = "ok";
	clock-frequency = <400000>;
	mediatek,use-open-drain;
	pn547@2B {
		compatible = "pn547";
		reg = <0x2B>;
		interrupt-parent = <&pio>;
		interrupts = <3 0>;
		pn547,irq-gpio = <&pio 3 0>;
		pn547,ven-gpio = <&pio 159 0>;
		pn547,firm-gpio = <&pio 158 0>;
		pn547,pvdd-gpio = <&expander_gpios0 10 0>;
	};
};
